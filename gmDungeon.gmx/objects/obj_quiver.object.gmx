    while(ini_key_exists(base64_encode("Player Inventory"), string(b) + " weaponType"))
    {
        type = base64_decode(ini_read_string(base64_encode("Player Inventory"), string(b) + " weaponType",""))
        if (type == "thrown" || type == "arrow" || type == "bolt")
        {
            draw_text(32,(32 * (b + 2)) + 32, chr(b + 97) + " " + base64_decode(ini_read_string(base64_encode("Player Inventory"), string(b) + " name", "")));
